{
  "name": "@e7f3/frontend-config",
  "version": "1.0.0",
  "description": "Shared frontend configuration presets and builders for Webpack, Jest, ESLint, and Stylelint",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./webpack": {
      "types": "./dist/webpack/index.d.ts",
      "require": "./dist/webpack/index.js",
      "import": "./dist/webpack/index.js"
    },
    "./jest": {
      "types": "./dist/jest/index.d.ts",
      "require": "./dist/jest/index.js",
      "import": "./dist/jest/index.js"
    },
    "./eslint": {
      "types": "./dist/eslint/index.d.ts",
      "require": "./dist/eslint/index.js",
      "import": "./dist/eslint/index.js"
    },
    "./stylelint": {
      "types": "./dist/stylelint/index.d.ts",
      "require": "./dist/stylelint/index.js",
      "import": "./dist/stylelint/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"Tests not yet implemented\" && exit 0"
  },
  "keywords": [
    "webpack",
    "jest",
    "eslint",
    "stylelint",
    "config",
    "preset",
    "frontend",
    "build-tools"
  ],
  "author": "e7f3",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0",
    "jest": "^29.0.0",
    "stylelint": "^15.0.0 || ^16.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "eslint": {
      "optional": true
    },
    "stylelint": {
      "optional": true
    },
    "jest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@svgr/webpack": "^8.1.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^20.10.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.4.0",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "html-webpack-plugin": "^5.6.5",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss-loader": "^8.2.0",
    "sass-loader": "^16.0.6",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-semantic-groups": "^1.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.3.0",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "babel-jest": "^29.7.0",
    "compression-webpack-plugin": "^11.0.0",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e7f3/frontend-config.git"
  },
  "bugs": {
    "url": "https://github.com/e7f3/frontend-config/issues"
  },
  "homepage": "https://github.com/e7f3/frontend-config#readme"
}
